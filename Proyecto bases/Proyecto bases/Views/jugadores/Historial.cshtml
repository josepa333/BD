

@model IEnumerable<Proyecto_bases.Models.infoJugador_Result>

@{
    ViewBag.Title = "Historial jugador";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Nombre")
        </th>
        <th>
            @Html.DisplayName("Fecha inicio")
        </th>
        <th>
            @Html.DisplayName("Fecha final")
        </th>
        <th>
            @Html.DisplayName("Tipo")
        </th>
        <th>
            @Html.DisplayName("Promedio rendimiento")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <th>
                @Html.DisplayFor(model => item.nombre)
            </th>
            <th>
                @Html.DisplayFor(model => item.fchinicio)
            </th>
            <th>
                @Html.DisplayFor(model => item.fchfinal)
            </th>
            <th>
                @Html.DisplayFor(model => item.tipo)
            </th>
            <th>
                @Html.DisplayFor(model => item.rendimiento)
            </th>

        </tr>
    }

</table>