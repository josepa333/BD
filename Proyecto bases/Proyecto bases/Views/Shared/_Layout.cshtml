
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/jqueryui")
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
</head>



<body>
    <style>
        h2 {
            color: #9CA6AD
        }
    </style>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:#1A2A40">
        <img src="~/Resources/FIFA.svg.png" title="title" alt="IMAGES" width="100" height="50" />
        <div class="container">
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-pills">
                    <li role="presentation"><a href=" /Home/Index" style="color:#9CA6AD"> Home</a></li>
                    <li>@Html.ActionLink("Consultas", "Consultas","Home", null, new { style = "color:#9CA6AD" })</li>
                    <li>@Html.ActionLink("Mantenimiento", "Mantenimiento", "Home", null, new { style = "color:#9CA6AD" })</li>
                    <li>@Html.ActionLink("About", "About", "Home", null, new { style = "color:#9CA6AD" })</li>
                    <li>@Html.ActionLink("Contact Us", "Contact", "Home", null, new { style = "color:#9CA6AD" })</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Fifa TEC.</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>