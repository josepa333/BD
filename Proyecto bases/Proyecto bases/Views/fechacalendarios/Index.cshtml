@model IEnumerable<Proyecto_bases.Models.fechacalendario>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Fechas calendario</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("ID Calendario")
        </th>
        <th>
            @Html.DisplayName("Fecha")
        </th>
        <th>
            @Html.DisplayName("ID Competicion")
        </th>
        <th>
            @Html.DisplayName("ID Temporada")
        </th>
        <th>
            @Html.DisplayName("Fecha creacion")
        </th>
        <th>
            @Html.DisplayName("Fecha modificacion")
        </th>
        <th>
            @Html.DisplayName("Usuario creador")
        </th>
        <th>
            @Html.DisplayName("Usuario modificador")
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idcalendario)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.idcompeticion)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.idtemporada)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.fchcreacion)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.fchmodificacion)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.usrcreador)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.usrmodificador)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.idcalendario }) |
            @Html.ActionLink("Details", "Details", new { id = item.idcalendario }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.idcalendario }) |
            @Html.ActionLink("Juegos", "juegosFechaCalendario", new { idC = item.idcalendario })
        </td>

    </tr>
}

</table>
