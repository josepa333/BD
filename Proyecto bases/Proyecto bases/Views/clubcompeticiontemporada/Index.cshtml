@model IEnumerable<Proyecto_bases.Models.clubcompeticiontemporada>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Clubes de competiciones-temporadas</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th style="background-color:#1A2A40; color:#9CA6AD ">
            @Html.DisplayNameFor(model => model.idcompeticion)
        </th>
        <th style="background-color:#1A2A40; color:#9CA6AD ">
            @Html.DisplayNameFor(model => model.idtemporada)
        </th>
        <th style="background-color:#1A2A40; color:#9CA6AD ">
            @Html.DisplayNameFor(model => model.idclub)
        </th>
        <th style="background-color:#1A2A40; color:#9CA6AD ">
            @Html.DisplayNameFor(model => model.fchcreacion)
        </th>
        <th style="background-color:#1A2A40; color:#9CA6AD ">
            @Html.DisplayNameFor(model => model.fchmodificacion)
        </th>
        <th style="background-color:#1A2A40; color:#9CA6AD ">
            @Html.DisplayNameFor(model => model.usrcreador)
        </th>
        <th style="background-color:#1A2A40; color:#9CA6AD ">
            @Html.DisplayNameFor(model => model.usrmodificador)
        </th>
        <th style="background-color:#1A2A40; color:#9CA6AD "></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td style="background-color:#f2f2f2">

            @Html.DisplayFor(modelItem => item.idcompeticion)
        </td>
        <td style="background-color:#e0eeFF">
            @Html.DisplayFor(modelItem => item.idtemporada)
        </td>
        <td style="background-color:#f2f2f2">

            @Html.DisplayFor(modelItem => item.idclub)
        </td>
        <td style="background-color:#e0eeFF">
            @Html.DisplayFor(modelItem => item.fchcreacion)
        </td>
        <td style="background-color:#f2f2f2">

            @Html.DisplayFor(modelItem => item.fchmodificacion)
        </td>
        <td style="background-color:#e0eeFF">
            @Html.DisplayFor(modelItem => item.usrcreador)
        </td>
        <td style="background-color:#f2f2f2">

            @Html.DisplayFor(modelItem => item.usrmodificador)
        </td>
        <td style="background-color:#e0eeFF">
            @Html.ActionLink("Edit", "Edit", new { id = item.idclub, id2 = item.idtemporada, id3 = item.idcompeticion }) |
            @Html.ActionLink("Details", "Details", new { id = item.idclub, id2 = item.idtemporada, id3 = item.idcompeticion }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.idclub, id2 = item.idtemporada, id3 = item.idcompeticion })
        </td>
    </tr>
}

</table>
