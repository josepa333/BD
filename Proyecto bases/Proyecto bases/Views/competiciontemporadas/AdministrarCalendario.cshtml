@model IEnumerable<Proyecto_bases.Models.fechacalendario>

@{
                /**/
                ViewBag.Title = "Index";
                Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Calendario de juegos de temporada @ViewBag.idT y competicion @ViewBag.idC</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idcalendario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fchcreacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fchmodificacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usrcreador)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usrmodificador)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idcalendario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fchcreacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fchmodificacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.competiciontemporada.usrcreador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usuario.usrcreador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usuario1.usrcreador)
        </td>
    </tr>
}

</table>
