@model IEnumerable<Proyecto_bases.Models.juego>

@{
    ViewBag.Title = "Resultados de partidos jugados";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("ID Juego")
        </th>
        <th>
            @Html.DisplayName("ID Club Casa")
        </th>
        <th>
            @Html.DisplayName("Goles Casa")
        </th>
        <th>
            @Html.DisplayName("ID Club Visita")
        </th>
        <th>
            @Html.DisplayName("Goles Visita")
        </th>
        <th>
            @Html.DisplayName("ID Calendario")
        </th>
        <th>
            @Html.DisplayName("Sinopsis")
        </th>
        <th></th>
    </tr>

    @foreach (var row in ViewBag.data) 
    {                                   
    <tr>
        <td>@row.Fieldx</td>
    </tr>
    }

    @foreach (var item in Model)
    {
        <tr>

            <th>
                @Html.DisplayFor(model => item.idjuego)
            </th>
            <th>
                @Html.DisplayFor(model => item.idclubcasa)
            </th>
            <th>
                @Html.DisplayFor(model => item.golescasa)
            </th>
            <th>
                @Html.DisplayFor(model => item.idclubvisita)
            </th>
            <th>
                @Html.DisplayFor(model => item.golesvisita)
            </th>
            <th>
                @Html.DisplayFor(model => item.idcalendario)
            </th>
            <th>
                @Html.DisplayFor(model => item.sinopsis)
            </th>
        </tr>
    }

</table>